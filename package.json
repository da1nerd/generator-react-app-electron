{
  "name": "generator-react-app-electron",
  "version": "1.0.0",
  "description": "A \"Create React App\" project configured for electron.",
  "homepage": "https://github.com/neutrinog/react-app-electron-template",
  "author": {
    "name": "Joel Lonbeck",
    "email": "joel@neutrinographics.com",
    "url": "http://neutrinographics.com"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "create-react-app",
    "electron",
    "yeoman-generator",
    "yeoman",
    "generator",
    "react",
    "boilerplate"
  ],
  "devDependencies": {
    "coveralls": "^3.0.2",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-config-xo": "^0.25.1",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "lint-staged": "^8.2.1",
    "prettier": "^1.15.2",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.9.1"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "fs-extra": "^7.0.1",
    "inquirer-npm-name": "^3.0.0",
    "mkdirp": "^0.5.1",
    "yeoman-generator": "^2.0.5",
    "yosay": "^2.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/generators/"
    ],
    "collectCoverage": true
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest"
  },
  "repository": "neutrinog/generator-react-app-electron",
  "license": "MIT"
}
